pipeline
{
    agent 
    { 
        label 'ubuntu'
    }
    stages 
    {
        stage('package') 
        {
            steps 
            {
                git url: 'https://github.com/tarunkumarpendem/spring-petclinic.git', 
                    branch: 'REL_1.0'
            }
        }    
        stage('build')
        {   
            steps
            {
               script
               { 
                sh '/usr/share/maven/bin/mvn clean package install test'
               }
            
            }
              
        }
    }
}

